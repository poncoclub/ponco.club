---
import Layout from '../layouts/Layout.astro';
import Hero from '../components/Hero.astro';
import Navi from '../components/Navi.astro';
import Footer from '../components/Footer.astro';

const jtitle = "ポンコウクラブ";
const etitle = "PONCO CLUB";
const description = "六本木交差点を中心に関東圏で活動する無線クラブです。";
---

<Layout title={jtitle} description={description}>
  <Navi title={etitle} />
  <Hero title={jtitle} description={description} />
  <Footer title={etitle} />
</Layout>
